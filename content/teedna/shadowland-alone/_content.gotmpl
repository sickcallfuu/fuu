{{ range resources.Match "images/teedna/shadowland-alone/thumb/**" }}

  {{ $pagePath := path.BaseName .Name }}
  {{ $pageTitle := path.BaseName .Name }}
  {{ $.AddPage (dict "kind" "page" "path" $pagePath "title" $pageTitle) }}

  {{ $resourcePath := printf "%s/shadowland-alone.%s" $pagePath .MediaType.FirstSuffix.Suffix }}
  {{ $content := dict "mediaType" .MediaType.Type "value" . }}
  {{ $resource := dict "content" $content "path" $resourcePath }}
  {{ $.AddResource $resource }}

{{ end }}